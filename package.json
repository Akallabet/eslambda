{
  "name": "eslambda",
  "version": "0.2.0",
  "description": "zero dependencies functional programming utilites for javascript",
  "main": "dist/eslambda.js",
  "scripts": {
    "lint": "eslint src --ext .js -c .eslintrc",
    "prettier": "prettier src/**/*.js",
    "lint:fix": "eslint src --ext .js -c .eslintrc --fix",
    "test": "jest --watch --maxWorkers=25%",
    "test:cli": "jest --maxWorkers=50%",
    "build:es": "BABEL_ENV=es babel src/index.js -o dist/eslambda.es.js",
    "build:cjs": "BABEL_ENV=cjs babel src/index.js -o dist/eslambda.js",
    "build": "npm run build:es && npm run build:cjs",
    "clean": "rimraf dist/*",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "files": [
    "README.md",
    "dist"
  ],
  "keywords": [
    "javascript",
    "ecmascript",
    "fp",
    "functional programming"
  ],
  "author": "Akallabet",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@types/jest": "^25.1.3",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-preval": "^5.0.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-jest": "^23.8.0",
    "husky": "^4.3.6",
    "jest": "^26.6.0",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2"
  }
}
