{
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "extends": ["eslint:recommended", "eslint-config-prettier", "plugin:jest/recommended"],
  "rules": {
    "strict": ["error", "never"],
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
  },
  "env": {
    "node": true,
    "es2021": true
  }
}
